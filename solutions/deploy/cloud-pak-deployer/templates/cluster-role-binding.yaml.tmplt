kind: ClusterRoleBinding
apiVersion: rbac.authorization.k8s.io/v1
metadata:
  name: ${cloud_pak_deployer_cluster_role_binding_name}
subjects:
  - kind: ServiceAccount
    name: ${cloud_pak_deployer_service_account_name}
    namespace: ${cloud_pak_deployer_namespace_name}
roleRef:
  apiGroup: rbac.authorization.k8s.io
  kind: ClusterRole
  name: ${cloud_pak_deployer_cluster_role_name}
